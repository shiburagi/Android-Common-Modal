<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0px"
        android:left="0px"
        android:right="0px"
        android:top="0px">
        <shape android:shape="rectangle">
            <stroke android:width="1dp"
                android:color="@color/colorSemiTransparentLine" />
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorSemiTransparent" />
        </shape>
    </item>
</layer-list>

